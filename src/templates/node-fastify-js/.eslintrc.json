{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": "error",
      "no-console": "off",
      "import/prefer-default-export": "warn",
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "_"
        }
      ]
    }
}
